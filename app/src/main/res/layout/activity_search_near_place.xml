<?xml version="1.0" encoding="utf-8"?>
<com.rey.material.widget.FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.SearchNearPlace">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.rey.material.widget.LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom">


        <com.rey.material.widget.Button
            android:id="@+id/btnRestorentFind"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:enabled="false"
            android:onClick="onRestorentFindClick"
            android:text="Nearby Pharmacyies" />

        <com.rey.material.widget.Button
            android:id="@+id/btnHospitalFind"
            android:onClick="onHospitalsFindClick"
            android:text="Nearby Hospitals"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:enabled="false"
            android:layout_height="wrap_content"/>

    </com.rey.material.widget.LinearLayout>

</com.rey.material.widget.FrameLayout>
